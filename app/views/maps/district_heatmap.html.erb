<%= javascript_include_tag "district_map" %>
<h1 id="title">Open Street Crimes</h1>

<%= content_tag "div", id: 'map', class: 'smallmap', data: {district: @districts, center_x: @center.x, center_y: @center.y, quantils: @quantils } do %>
<% end %>
<div id="docs">
  <table>
    <tr>
      <th>Color</th>
      <th>Value</th>
    </tr>
    <% @quantils.each do |quantil| %>
      <tr>
        <td>Color</td>
        <td><= <%= quantil %> </td>
      </tr>
    <% end %>
      <tr>
        <td>Color</td>
        <td>> <%= @quantils.last %> </td>
      </tr>
  </table>
    <p>Maybe put some statistics right here.
    </p>
  <table>
    <tr>
      <th>Name</th>
      <th>Count</th>
    </tr>
    <% @districts.each do |district| %>
      <tr>
        <td><%= district['name'] %></td>
        <td><%= district['count'] %></td>
      </tr>
    <% end %>
  </table>
  </div>
